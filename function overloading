#include <iostream>
using namespace std;
class maths{
    public:
    int sum(int a,int b){
        return a+b;
        
    }
    int sum(int a,int b, int c){
        return a+b+c;
    }
    int sum(int a,float b){
        return a+b;
        
    }
};
int main(){
    maths m1;
    cout<<m1.sum(9,8)<<endl;
    return 0;
}
